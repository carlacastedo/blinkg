@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.org/#> .

ex:TriplesMap1 a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ious.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://example.org/{fname}_{lname}";
        rr:class ex:Person
    ];
    rr:predicateObjectMap [
        rr:predicate ex:firstname;
        rr:objectMap [ rml:reference "fname" ]
    ];
    rr:predicateObjectMap [
        rr:predicate ex:lastname;
        rr:objectMap [ rml:reference "lname" ]
    ];
    rr:predicateObjectMap [
        rr:predicate ex:owes;
        rr:objectMap [ rml:reference "amount"; rr:datatype xsd:double ]
    ].
