@prefix : <http://vocab.gtfs.org/terms#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix gtfs: <http://vocab.gtfs.org/terms#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix schema: <http://schema.org/> .
@prefix status: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@base <http://vocab.gtfs.org/terms#> .

<http://vocab.gtfs.org/terms#> rdf:type owl:Ontology ;
                                dct:creator [ foaf:mbox "Pieter.Colpaert@UGent.be" ;
                                              foaf:name "Pieter Colpaert"
                                            ] ,
                                            [ foaf:mbox "abyrd@conveyal.com" ;
                                              foaf:name "Andrew Byrd"
                                            ] ;
                                dct:description "Esta ontología es una traducción de la Especificación General de Feeds de Transporte Público a URIs. El objetivo es crear una plataforma de intercambio donde el modelo Linked GTFS pueda usarse para obtener los datos correctos en el formato adecuado. Para la especificación semántica de clases y propiedades, visite https://developers.google.com/transit/gtfs/reference."@es ,
                                                "This ontology is a translation of the General Transit Feed Specification towards URIs. Its intended use is creating an exchange platform where the Linked GTFS model can be used as a start to get the right data into the right format. For semantics of the classes and properties, see https://developers.google.com/transit/gtfs/reference."@en ;
                                vann:preferredNamespacePrefix "gtfs" ;
                                vann:preferredNamespaceUri "http://vocab.gtfs.org/terms#" ;
                                rdfs:label "Especificación General de Feeds de Transporte Público"@es ,
                                           "General Transit Feed Specification"@en ;
                                rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference> .


#################################################################
#    Object Properties
#################################################################

gtfs:agency rdf:type owl:ObjectProperty ;
            rdfs:domain gtfs:Route ;
            rdfs:range gtfs:Agency ;
            rdfs:comment "Enlaza una ruta a una agencia de la que es parte."@es ,
                         "Links a route to an agency that is part of."@en ;
            rdfs:label "agency"@en ,
                       "empresa"@es .


gtfs:continuousDropOff rdf:type owl:ObjectProperty ;
                       rdfs:domain gtfs:Route ;
                       rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                          owl:hasValue <http://transport.linkeddata.es/kos/drop-off> ;
                                                        ]
                                                     ) ;
                                    rdf:type owl:Class
                                ] ;
                       rdfs:comment "Indica si se bajan los pasajeros en cualquier punto del trayecto del vehículo."@es ,
                                    "Indicates that the rider can alight the transit vehicle at any point along the vehicle’s travel path."@en ;
                       rdfs:label "Continuous Drop Off"@en ,
                                  "Bajada continua"@es.


gtfs:continuousPickup rdf:type owl:ObjectProperty ;
                      rdfs:domain gtfs:Route ;
                      rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                          owl:hasValue <http://transport.linkeddata.es/kos/pickup> ;
                                                        ]
                                                     ) ;
                                    rdf:type owl:Class
                                ] ;
                      rdfs:comment "Indica si se recogen los pasajeros en cualquier punto del trayecto del vehículo."@es ,
                                   "Indicates that the rider can board the transit vehicle at any point along the vehicle’s travel path."@en ;
                      rdfs:label "Continuous Pickup"@en ,
                                 "Recogida continua"@es.


gtfs:dropOffType rdf:type owl:ObjectProperty ;
                 rdfs:domain gtfs:StopTime ;
                 rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                    owl:hasValue <http://transport.linkeddata.es/kos/drop-off> ;
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                            ] ;
                 rdfs:comment "Indica si los pasajeros se bajan en una parada como parte del horario normal o si no pueden bajar en esa parada."@es ,
                              "Indicates drop off method."@en ;
                 rdfs:label "Drop off type"@en ,
                            "tipo de bajada"@es .


gtfs:parentStation rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( gtfs:Stop
                                    gtfs:StationEntrance
                                    gtfs:GenericNode
                                    gtfs:BoardingArea
                                  )
                    ] ;
                   rdfs:range gtfs:Station ;
                   rdfs:comment "Defines hierarchy between the different locations"@en ,
                                "Define una jerarquía entre las distintas localizaciones."@es ;
                   rdfs:label "Parent Station"@en ,
                              "estación matriz"@es .


gtfs:pickupType rdf:type owl:ObjectProperty ;
                rdfs:domain gtfs:StopTime ;
                rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                    owl:hasValue <http://transport.linkeddata.es/kos/pickup> ;
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                            ] ;
                rdfs:comment "Indica si se recogen los pasajeros en una parada como parte del horario normal o si su recogida en dicha parada no se encuentra disponible."@es ,
                             "Indicates pickup method."@en ;
                rdfs:label "Pickup type"@en ,
                           "tipo de recogida"@es .


gtfs:route rdf:type owl:ObjectProperty ;
           rdfs:domain gtfs:Trip ;
           rdfs:range gtfs:Route ;
           rdfs:comment "Este viaje forma parte de esta ruta."@es ,
                        "This trip is applicable to this route."@en ;
           rdfs:label "Route"@en ,
                      "Ruta"@es .


gtfs:routeType rdf:type owl:ObjectProperty ;
               rdfs:domain gtfs:Route ;
               rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                   owl:hasValue <http://transport.linkeddata.es/kos/route-type> ;
                                                  ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Enlaza al tipo de vehículo que opera en esta ruta"@es ,
                            "Indicates the type of transportation used on a route."@en ;
               rdfs:label "Route type"@en ,
                          "Tipo de ruta"@es ;
               rdfs:seeAlso <https://developers.google.com/transit/gtfs/examples/display-to-users#RouteType> .


gtfs:service rdf:type owl:ObjectProperty ; 
             rdfs:domain gtfs:Trip ;
             rdfs:range gtfs:Service ;
             rdfs:comment "Identifica un conjunto de fechas cuando el servicio está disponible para uno o más viajes."@es ,
                          "Identifies a set of dates when service is available for one or more trips."@en ;
             rdfs:label "Service"@en ,
                        "Servicio"@es .


gtfs:serviceRule rdfs:comment "Reglas de servicio que definen un conjunto de fechas."@es ,
                              "Service rules which define together a set of dates."@en ;
                 rdfs:label "Service rule"@en ,
                            "Regla de servicio"@es ;
                 rdf:type owl:ObjectProperty ;
                 rdfs:domain gtfs:Service ;
                 rdfs:range gtfs:ServiceRule .


gtfs:shape rdf:type owl:ObjectProperty ;
           rdfs:domain gtfs:Trip ;
           rdfs:range gtfs:Shape ;
           rdfs:comment "Identifies a geospatial shape that describes the vehicle travel path for a trip."@en ,
                        "Identifica una forma geoespacial que describe el trayecto del vehículo para un viaje."@es ;
           rdfs:label "Shape"@en ,
                      "Trayecto"@es .


gtfs:shapePoint rdf:type owl:ObjectProperty ;
                rdfs:domain gtfs:Shape ;
                rdfs:range gtfs:ShapePoint ;
                rdfs:comment "Cada uno de los puntos geoespaciales que son parte del recorrido de un trayecto."@es ,
                             "Each one of the geospatial points that are part of a shape."@en ;
                rdfs:label "Shape Point"@en ,
                           "Punto de recorrido"@es .


gtfs:stop rdf:type owl:ObjectProperty ;
          rdfs:domain gtfs:StopTime ;
          rdfs:range gtfs:Stop ;
          rdfs:comment "Indica la parada correspondiente en un instante de tiempo."@es ,
                       "Indicates the corresponding stop in a point of time."@en ;
          rdfs:label "Stop"@en ,
                     "Parada"@es .


gtfs:trip rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( gtfs:StopTime 
                                      gtfs:Frequency 
                                    )
                      ] ;
          rdfs:range gtfs:Trip ;
          rdfs:comment "Indica que este concepto es parte de cierto viaje."@es ,
                       "Indicates that this is part of a certain trip."@en ;
          rdfs:label "Trip"@en ,
                     "Viaje"@es .


gtfs:monday rdf:type owl:ObjectProperty ;
            rdfs:domain gtfs:CalendarRule ;
            rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
            rdfs:comment "Service is available on all Mondays in the duration of the service calendar."@en ,
                         "Servicio disponible todos los lunes en la duración del calendario de servicio."@es ;
            rdfs:label "Monday"@en ,
                       "Lunes"@es .


gtfs:tuesday rdf:type owl:ObjectProperty ;
             rdfs:domain gtfs:CalendarRule ;
             rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
             rdfs:comment "Service is available on all Tuesdays in the duration of the service calendar."@en ,
                          "Servicio disponible todos los martes en la duración del calendario de servicio."@es ;
             rdfs:label "Tuesday"@en ,
                        "Martes"@es .


gtfs:wednesday rdf:type owl:ObjectProperty ;
               rdfs:domain gtfs:CalendarRule ;
               rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
               rdfs:comment "Service is available on all Wednesdays in the duration of the service calendar."@en ,
                            "Servicio disponible todos los miércoles en la duración del calendario de servicio."@es ;
               rdfs:label "Wednesday"@en ,
                          "Miércoles"@es .


gtfs:thursday rdf:type owl:ObjectProperty ;
              rdfs:domain gtfs:CalendarRule ;
              rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
              rdfs:comment "Service is available on all Thursdays in the duration of the service calendar."@en ,
                           "Servicio disponible todos los jueves en la duración del calendario de servicio."@es ;
              rdfs:label "Thursday"@en ,
                         "Jueves"@es .


gtfs:friday rdf:type owl:ObjectProperty ;
            rdfs:domain gtfs:CalendarRule ;
            rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
            rdfs:comment "Service is available on all Fridays in the duration of the service calendar."@en ,
                         "Servicio disponible todos los viernes en la duración del calendario de servicio."@es ;
            rdfs:label "Friday"@en ,
                       "Viernes"@es .


gtfs:saturday rdf:type owl:ObjectProperty ;
              rdfs:domain gtfs:CalendarRule ;
              rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
              rdfs:comment "Service is available on all Saturdays in the duration of the service calendar."@en ,
                           "Servicio disponible todos los sábados en la duración del calendario de servicio."@es ;
              rdfs:label "Saturday"@en ,
                         "Sábado"@es .


gtfs:sunday rdf:type owl:ObjectProperty ;
            rdfs:domain gtfs:CalendarRule ;
            rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/day> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
            rdfs:comment "Service is available on all Sundays in the duration of the service calendar."@en ,
                         "Servicio disponible todos los domingos en la duración del calendario de servicio."@es ;
            rdfs:label "Sunday"@en ,
                       "Domingo"@es .


gtfs:exceptionType rdf:type owl:ObjectProperty ;
                   rdfs:domain gtfs:CalendarDateRule ;
                   rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                        owl:hasValue <http://transport.linkeddata.es/kos/exception-type> ;
                                                      ]
                                                    ) ;
                                rdf:type owl:Class
                              ] ;
                   rdfs:comment "Indicates whether service is available on the date specified in the date field. Valid options are: service added or removed."@en ;
                   rdfs:label "Exception Type"@en .


gtfs:usesExactTimes rdf:type owl:ObjectProperty ;
                    rdfs:domain gtfs:Frequency ;
                    rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                owl:hasValue <http://transport.linkeddata.es/kos/exact-times> ;
                                              ]
                                            ) ;
                         rdf:type owl:Class
                        ] ;
                    gtfs:comment "Determina si los viajes basados en frecuencias deben estar programados de manera exacta según la información especificada de tiempo entre viajes."@es ,
                                 "Indicates the type of service for a trip. Frequency-based service in which service does not follow a fixed schedule throughout the day. Schedule-based trips with the exact same headway throughout the day."@en ;
                    rdfs:label "Frecuency type"@en ,
                               "Tipo de frecuencia"@es .


gtfs:locationType rdf:type owl:ObjectProperty ;
                  rdfs:domain gtfs:Location ;
                  rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                      owl:hasValue <http://transport.linkeddata.es/kos/location-type> ;
                                                    ]
                                                  ) ;
                                rdf:type owl:Class
                              ] ;
                  rdfs:comment "Location type. Stop, Station, Entrance/Exit, Generic Node, Boarding Area."@en ,
                               "Tipo de localización. Parada, Estación, Entrada/Salida, Nodo Genérico, Área de embarque."@es;
                  rdfs:label "Location Type"@en ,
                             "Tipo de localización"@es .


gtfs:wheelchairAccessible rdf:type owl:ObjectProperty ;
                          rdfs:domain [rdf:type owl:Class; 
                                       owl:unionOf (gtfs:Station
                                                    gtfs:Trip
                                                    gtfs:Stop
                                                    gtfs:StationEntrance
                                                   )
                                      ];
                          rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                          owl:hasValue <http://transport.linkeddata.es/kos/wheelchair-accesible> ;
                                        ]
                                        ) ;
                                        rdf:type owl:Class
                                      ] ;
                          gtfs:comment "Si wheelchair_accessible es 0, mapee a gtfs:CheckParentStation; si es 1, mapee a gtfs:WheelchairAccessible; si es 2, mapee a gtfs:NotWheelchairAccessible."@es ,
                                       "When wheelchair_accessible is 0, map to gtfs:CheckParentStation; if 1 map to gtfs:WheelchairAccessible; if 2 map to gtfs:NotWheelchairAccessible."@en ;
                          rdfs:comment "La parada o estación específica es accesible para silla de ruedas. No hay información, accesible o inaccesible."@es ,
                                       "Wheelchair boardings are possible from the specified stop or station. No information, accesible or inaccesible"@en ;
                          rdfs:label "Accesible para silla de ruedas"@es ,
                                     "Wheelchair accesible"@en .


gtfs:timepoint rdf:type owl:ObjectProperty ;
               rdfs:domain gtfs:StopTime ;
               rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                    owl:hasValue <http://transport.linkeddata.es/kos/timepoint> ;
                                                  ]
                                                ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Indicates if arrival and departure times for a stop are strictly adhered to by the vehicle or if they are instead approximate and/or interpolated times. Approximate times or exact times."@en ,
                            "Indica si los tiempos de llegada y salida para una parada se siguen extrictamente o si se aproximan e interpolan. Tiempos aproximados o tiempos exactos."@es ;
               rdfs:label "Timepoint"@en .


gtfs:direction rdf:type owl:ObjectProperty ;
               rdfs:domain gtfs:Trip ;
               rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                           owl:hasValue <http://transport.linkeddata.es/kos/direction>;
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
               rdfs:comment "A binary property to indicate the direction the trip is going (outbound/inbound)."@en ,
                            "Apunta a un valor binario que indica la dirección del viaje (ida/vuelta)."@es ;
               rdfs:label "Direction"@en ,
                          "Dirección"@es .


gtfs:bikesAllowed rdf:type owl:ObjectProperty ;
                  rdfs:domain gtfs:Trip ;
                  rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                           owl:hasValue <http://transport.linkeddata.es/kos/bikes-allowed>;
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
                  rdfs:label "Bikes allowed"@en ,
                             "Bicicletas permitidas"@es ;
                  rdfs:comment "Indicates whether bikes are allowed. No information, allowed, not allowed."@en ,
                               "Indica si las bicicletas están permitidas. No hay información, permitidas o no permitidas."@es .


#################################################################
#    Data properties
#################################################################

#gtfs:Agency

gtfs:id rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( gtfs:Agency
                                    gtfs:Route
                                    gtfs:Shape
                                    gtfs:Service
                                    gtfs:Location
                                    gtfs:Trip
                                  )
                    ] ;
        rdfs:range xsd:string ;
        rdfs:subPropertyOf dct:identifier ;
        rdfs:comment "Identifies a transit agency, a route, a shape, a set of dates when service is available, a location, a trip or a zone for a stop."@en ,
                     "Identifica una agencia de tránsito, una ruta, un trayecto, un conjunto de fechas en las que un servicio está disponible, un viaje o una zona para una parada"@es ;
        rdfs:label "ID"@en ,
                   "ID"@es .


gtfs:name rdf:type owl:DatatypeProperty ;
          rdfs:domain [rdf:type owl:Class ;
                       owl:unionOf (gtfs:Agency
                                    gtfs:Location
                                   )
                       ];
          rdfs:range foaf:name ;
          rdfs:comment "Full name of the transit agency or the location."@en ,
                       "Nombre completo de la agencia de tránsito o de la localización"@es ;
          rdfs:label "Name"@en ,
                     "Nombre"@es .

 
gtfs:url rdf:type owl:DatatypeProperty ;
         rdfs:domain [rdf:type owl:Class ;
                      owl:unionOf (gtfs:Agency
                                   gtfs:Route
                                   gtfs:Location
                                   gtfs:Feed
                                  )
                      ];
         rdfs:range foaf:page ;
         rdfs:comment "URL of the transit agency, a particular route, a location, a publisher of a feed or a contact information URL."@en ,
                      "URL de la agencia de tránsito, una ruta particular, una localización, un editor de un feed o una URL de información de contacto."@es ;
         rdfs:label "URL"@en , 
                    "URL"@es .


gtfs:timezone rdf:type owl:DatatypeProperty ;
              rdfs:domain [rdf:type owl:Class ; 
                           owl:unionOf (gtfs:Agency
                                        gtfs:Location
                                      )
                           ];
              rdfs:range xsd:string ;
              rdfs:comment "Timezone of the agency or the location."@en ;
              rdfs:label "Timezone"@en .


gtfs:language rdf:type owl:DatatypeProperty ;
              rdfs:domain [rdf:type owl:Class; 
                           owl:unionOf (gtfs:Agency
                                        gtfs:Feed
                                       )
                           ];
              rdfs:range xsd:string ; 
              rdfs:subPropertyOf dct:language ;
              rdfs:comment "Language used by a transit agency, used for the text in the GTFS dataset or default language used when the data consumer doesn’t know the language of the rider."@en ;
              rdfs:label "Language"@en .


gtfs:phoneNumber rdf:type owl:DatatypeProperty ;
                 rdfs:domain gtfs:Agency ;
                 rdfs:range foaf:phone ;
                 rdfs:comment "A voice telephone number for the specified agency."@en ;
                 rdfs:label "PhoneNumber"@en .


gtfs:fareUrl rdfs:comment "La URL de una página web que permite a un usuario comprar online billetes u otros productos de transporte de esa empresa"@es ,
                          "The URL of a web page that allows a rider to purchase tickets or other fare instruments for that agency online"@en ;
             rdfs:label "URL de tarifas"@es ,
                        "fare URL"@en ;
             rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#agency_fields> ;
             rdf:type owl:DatatypeProperty ;
             rdfs:range foaf:page ;
             rdfs:domain gtfs:Agency .


#gtfs:Route

gtfs:longName rdfs:comment "Long name given to a route. This name is generally more descriptive than the gtfs:shortName and often includes the route's destination or stop."@en ,
                           "Nombre completo de una ruta"@es ;
              rdfs:label "Long name"@en ,
                         "nombre completo"@es ;
              rdfs:seeAlso <https://developers.google.com/transit/gtfs/examples/display-to-users#RouteLongName> ;
              rdf:type owl:DatatypeProperty ;
              rdfs:range [
                   a rdfs:Datatype ;
                   owl:onDatatype xsd:string;
                   owl:withRestrictions ([ xsd:pattern "^([A-Z][a-z]*)(\\s[A-Z][a-z]*)*$" ])
               ];
              rdfs:subPropertyOf dct:title ;
              rdfs:domain gtfs:Route .


gtfs:shortName rdfs:comment "Nombre corto dado a una ruta o viaje"@es ,
                            "Short name given to a route or a trip"@en ;
               rdfs:label "Short name"@en ,
                          "nombre corto"@es ;
               rdfs:seeAlso <https://developers.google.com/transit/gtfs/examples/display-to-users#RouteShortName> ;
               rdf:type owl:DatatypeProperty ;
               rdfs:range [
                   a rdfs:Datatype ;
                   owl:onDatatype xsd:string;
                   owl:withRestrictions ([ xsd:pattern "^([A-Z][a-z]*)(\\s[A-Z][a-z]*)*$" ])
               ];
               rdfs:subPropertyOf dct:title ;
               rdfs:domain [rdf:type owl:Class; 
                            owl:unionOf (gtfs:Route
                                         gtfs:Trip
                                        )
                           ].      


gtfs:desc rdf:type owl:DatatypeProperty ;
          rdfs:domain [rdf:type owl:Class; 
                            owl:unionOf (gtfs:Route
                                         gtfs:Location
                                        )
                           ];
          rdfs:range xsd:string ;
          rdfs:subPropertyOf dct:description ;
          rdfs:comment "Description of a route or a location that provides useful, quality information."@en ;
          rdfs:label "RouteDesc"@en .


gtfs:color rdf:type owl:DatatypeProperty ;
           rdfs:domain gtfs:Route ;
           rdfs:range xsd:string ;
           rdfs:comment "A 6 character hexidecimal color (without #) that matches the route color in public facing material"@en ,
                        "Un color expresado con 6 caracteres hexadecimales (sin #)"@es ;
           rdfs:label "Color"@en ,
                      "color"@es .


gtfs:textColor rdf:type owl:DatatypeProperty ;
               rdfs:domain gtfs:Route ;
               rdfs:range xsd:string ;
               rdfs:comment "A 6 character hexidecimal color (without #) asigned to a text label that is legible when written on top of a background of gtfs:color."@en ,
                            "Un color expresado con 6 caracteres hexadecimales (sin #) asignado a una etiqueta de texto."@es ;
               rdfs:label "Text color"@en ,
                          "color de texto"@es .


gtfs:sortOrder rdf:type owl:DatatypeProperty ;
               rdfs:domain gtfs:Route ;
               rdfs:range xsd:nonNegativeInteger ;
               rdfs:comment "Orders the routes in a way which is ideal for presentation to customers."@en ;
               rdfs:label "Sort Order"@en .


#gtfs:Shape,gtfs:ShapePoint

gtfs:nonNegativeFloat a rdfs:Datatype ;
                        rdfs:subClassOf xsd:float ;
                        owl:onDatatype xsd:float ;
                        owl:withRestrictions (
                                                [ xsd:minInclusive "0.0"^^xsd:float ]
                                              ) .


gtfs:distanceTraveled rdf:type owl:DatatypeProperty ;
                      rdfs:domain [rdf:type owl:Class; 
                           owl:unionOf (gtfs:ShapePoint
                                        gtfs:StopTime
                                       )
                           ];
                      rdfs:range gtfs:nonNegativeFloat ;
                      rdfs:comment "Indica la distancia a una parada desde el primer punto del recorrido. Representa una distancia de viaje real durante la ruta en unidades expresadas como pies o kilómetros. Esta información permite que quien planifica el viaje determine la porción de la forma que se debe trazar al mostrar parte de un viaje en el mapa. Los valores utilizados para gtfs:distanceTraveled se deben incrementar junto con los de gtfs:stopSequence, es decir, no se pueden usar para mostrar el recorrido inverso a lo largo de una ruta."@es ,
                                   "Positions a stop as a distance from the first shape point. It represents a real distance traveled along the route in units such as feet or kilometers. This information allows the trip planner to determine how much of the shape to draw when showing part of a trip on the map. The values used for gtfs:distanceTraveled must increase along with gtfs:stopSequence: they cannot be used to show reverse travel along a route."@en ;
                      rdfs:label "Distance Traveled"@en ,
                                 "distancia recorrida"@es .


gtfs:latitude rdf:type owl:DatatypeProperty ;
              rdfs:domain [rdf:type owl:Class; 
                           owl:unionOf (gtfs:ShapePoint
                                        gtfs:Stop
                                       )
                           ];
              rdfs:range geo:lat ;
              rdfs:comment "Latitude of a shape point or a stop."@en ;
              rdfs:label "Latitude"@en .


gtfs:longitude rdf:type owl:DatatypeProperty ;
               rdfs:domain [rdf:type owl:Class; 
                            owl:unionOf (gtfs:ShapePoint
                                         gtfs:Stop
                                        )
                           ];
               rdfs:range geo:long ;
               rdfs:comment "Longitude of a shape point or a stop."@en ;
               rdfs:label "Longitude"@en .


gtfs:pointSequence rdf:type owl:DatatypeProperty ;
                   rdfs:domain gtfs:ShapePoint ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "El campo gtfs:pointSequence asocia la latitud y longitud de un punto de recorrido con su orden de secuencia en el recorrido. Los valores para gtfs:pointSequence deben ser números enteros positivos y crecientes conforme avanza el viaje."@es ,
                                "The gtfs:pointSequence field associates the latitude and longitude of a shape point with its sequence order along the shape. The values for gtfs:pointSequence must be non-negative integers, and they must increase along the trip."@en ;
                   rdfs:label "Point Sequence"@en ,
                              "secuencia de puntos"@es .


#gtfs:Service,gtfs:ServiceRule,gtfs:CalendarRule,gtfs:CalendarDateRule

gtfs:startDate rdf:type owl:DatatypeProperty ;
               rdfs:domain [rdf:type owl:Class; 
                            owl:unionOf (gtfs:CalendarRule
                                         gtfs:Feed
                                        )
                           ];
               rdfs:range schema:startDate ;
               rdfs:comment "Start service day for the service interval or start date for which the GTFS data is valid."@en ;
               rdfs:label "StartDate"@en .


gtfs:endDate rdf:type owl:DatatypeProperty ;
             rdfs:domain [rdf:type owl:Class; 
                          owl:unionOf (gtfs:CalendarRule
                                       gtfs:Feed
                                      )
                           ];
             rdfs:range schema:endDate ;
             rdfs:comment "End service day for the service interval or end date for which the GTFS data is valid."@en ;
             rdfs:label "EndDate"@en .


gtfs:date rdf:type owl:DatatypeProperty ;
          rdfs:domain gtfs:CalendarDateRule ;
          rdfs:range xsd:date ;
          rdfs:subPropertyOf dct:date ;
          rdfs:comment "Date when service exception occurs."@en ;
          rdfs:label "Date"@en .


#gtfs:Frequency 

gtfs:startTime rdf:type owl:DatatypeProperty ;
               rdfs:domain gtfs:Frequency ;
               rdfs:range schema:startTime ;
               rdfs:comment "Define la hora a la que empieza el servicio con la frecuencia especificada"@es ,
                            "Specifies the time at which service begins with the specified frequency"@en ;
               rdfs:label "Start Time"@en ,
                          "hora de inicio"@es .


gtfs:endTime rdf:type owl:DatatypeProperty ;
             rdfs:domain gtfs:Frequency ;
             rdfs:range schema:endTime ;
             rdfs:comment "Define la hora a la que el servicio cambia de frecuencia (o bien finaliza) en la primera parada del viaje."@es ,
                          "Indicates the time at which service changes to a different frequency (or ceases) at the first stop in the trip."@en ;
             rdfs:label "End Time"@en ,
                        "hora final"@es .


gtfs:headwaySeconds rdf:type owl:DatatypeProperty ;
                    rdfs:domain gtfs:Frequency ;
                    rdfs:range xsd:positiveInteger ;
                    rdfs:comment "Indica el período de tiempo (en segundos) entre salidas desde la misma parada (tiempo entre viajes) para este tipo de viaje, durante el intervalo de tiempo especificado mediante start_time y end_time."@es ,
                                 "Indicates the time between departures from the same stop (headway) for this trip type, during the time interval specified by gtfs:startTime and gtfs:endTime."@en ;
                    rdfs:label "Headway Seconds"@en ,
                               "tiempo entre viajes"@es .


#gtfs:Location,gtfs:Stop,gtfs:Station

gtfs:code rdf:type owl:DatatypeProperty ;
          rdfs:domain gtfs:Location ;
          rdfs:range xsd:string ;
          rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stops_fields> ;         
          rdfs:comment "Short text or a number that identifies the location for riders. These codes are often used in phone-based transit information systems or printed on signage to make it easier for riders to get information for a particular location. This field should be left empty for locations without a code presented to riders."@en ;
          rdfs:label "Location Code"@en .


#gtfs:StopTime

gtfs:arrivalTime rdf:type owl:DatatypeProperty ;
                 rdfs:domain gtfs:StopTime ;
                 rdfs:range schema:Time ;
                 rdfs:comment "Check the original specification for special cases"@en ,
                              "Compruebe la especificación original para casos especiales"@es ;
                 rdfs:label "Arrival time"@en ,
                            "hora de llegada"@es ;
                 rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stop_times_fields> .


gtfs:departureTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain gtfs:StopTime ;
                   rdfs:range schema:Time ;
                   rdfs:comment "Check the original specification for special cases"@en ,
                                "Compruebe la especificación original para casos especiales"@es ;
                   rdfs:label "Departure time"@en ,
                              "hora de salida"@es ;
                   rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stop_times_fields> .


gtfs:stopSequence rdf:type owl:DatatypeProperty ;
                  rdfs:domain gtfs:StopTime ;
                  rdfs:range xsd:nonNegativeInteger ;
                  rdfs:comment "Indica el orden de paradas para un viaje concreto. Los valores de gtfs:stopSequence deben ser enteros no negativos, deben aumentar durante el viaje referenciado por la propiedad gtfs:trip."@es ,
                               "The stopSequence property identifies the order of the stops for a particular trip. The values must increase along the gtfs:Trip referenced in the gtfs:trip property."@en ;
                  rdfs:label "Stop sequence"@en ,
                             "secuencia de paradas"@es .


gtfs:headsign rdf:type owl:DatatypeProperty ;
              rdfs:domain [rdf:type owl:Class; 
                           owl:unionOf (gtfs:StopTime
                                        gtfs:Trip
                                       )
                           ];
              rdfs:range xsd:string ;
              rdfs:comment "Esta propiedad apunta al texto que indica el destino del viaje a los pasajeros."@es ,
                           "The gtfs:headsign property contains the text that appears on a sign that identifies the trip's destination to passengers."@en ;
              rdfs:label "Headsign"@en ,
                         "destino del viaje"@es ;
              rdfs:seeAlso <https://developers.google.com/transit/gtfs/examples/display-to-users#TripHeadsign> .


#gtfs:Trip

gtfs:block rdf:type owl:DatatypeProperty ;
           rdfs:domain gtfs:Trip ;
           rdfs:range xsd:string ;
           rdfs:subPropertyOf dct:identifier ;
           rdfs:comment "Identifies the block to which the trip within this feed belongs. A block consists of two or more sequential trips made using the same vehicle, where a passenger can transfer from one trip to the next just by staying in the vehicle."@en ,
                        "Identifica el bloque al que pertenece el viaje. Un bloque consta de dos o más viajes secuenciales realizados en el mismo vehículo, en los que un pasajero puede cambiar de viaje si simplemente permanece en el vehículo."@es ;
           rdfs:label "Block"@en ,
                      "Bloque"@es .


#gtfs:Feed

gtfs:publisher rdf:type owl:DatatypeProperty ;
               rdfs:domain gtfs:Feed ;
               rdfs:range xsd:string ;
               rdfs:subPropertyOf dct:publisher ;
               rdfs:comment "Full name of the organization that publishes the dataset."@en ;
               rdfs:label "Publisher"@en .


gtfs:version rdf:type owl:DatatypeProperty ;
             rdfs:domain gtfs:Feed ;
             rdfs:range schema:version ;
             rdfs:comment "String that indicates the current version of their GTFS dataset."@en ;
             rdfs:label "Version"@en .


#################################################################
#    Classes
#################################################################


gtfs:Agency rdf:type owl:Class ;
            rdfs:subClassOf foaf:Agent ;
            rdfs:comment "An agency operates a certain schedule based transport mode"@en ,
                         "Una empresa opera un cierto modo de transporte de manera programada"@es ;
            rdfs:label "Agency"@en ,
                       "Empresa"@es ;
            rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#agency_fields> .


gtfs:CalendarRule rdf:type owl:Class ;
                  rdfs:subClassOf gtfs:ServiceRule ;
                  gtfs:comment "a row from calendar.txt"@en ,
                               "una línea de calendar.txt"@es ;
                  rdfs:comment "Defines on which days of the week for a certain period the service is available."@en ,
                               "Define en qué días de la semana el servicio está disponible para un periodo concreto."@es ;
                  rdfs:label "Calendar rule"@en ,
                             "Regla de calendario"@es .


gtfs:CalendarDateRule rdf:type owl:Class ;
                      rdfs:comment "Defines exceptions for the services defined in gtfs:Service."@en ,
                                   "Indica la disponibilidad de servicio para una fecha concreta"@es ;
                      rdfs:label "Calendar date rule"@en ,
                                 "Regla de fecha de calendario"@es ;
                      rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#calendar_dates_fields> .


gtfs:Feed rdf:type owl:Class ;
          rdfs:subClassOf dcat:Dataset ;
          rdfs:comment "An instance of a gtfs:Feed is a linked GTFS feed that complies to this specification."@en ,
                       "Una instancia de gtfs:Feed es un feed GTFS enlazado que cumple con esta especificación."@es ;
          rdfs:label "GTFS Feed"@en ,
                     "GTFS Feed"@es ;
          rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#feed_info_fields> ;
          dcat:theme <http://eurovoc.europa.eu/2015> .


gtfs:Frequency rdf:type owl:Class ;
               rdfs:comment "An alternative to defining departures and arrivals as StopTimes: define frequencies for transit vehicle"@en ,
                            "Una alternativa a definir salidas y llegadas como StopTimes: definir frecuencias para vehículos de transporte público"@es ;
               rdfs:label "Frecuencia"@es ,
                          "Frequency"@en ;
               rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#frequencies_fields> .


gtfs:Location rdf:type owl:Class ;
              rdfs:comment "A location is a geographical point that has a latitude and a longitude."@en ;
              rdfs:label "Location"@en .


gtfs:Route rdf:type owl:Class ;
           rdfs:comment "A gtfs:Route is a commercial route followed entirely or partly by gtfs:Trips"@en ,
                        "Una gtfs:Route es un grupo de viajes (gtfs:Trips) que se muestran a los usuarios como servicio independiente"@es ;
           rdfs:label "Route"@en ,
                      "Ruta"@es ;
           rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#routes_fields> .


gtfs:Service rdf:type owl:Class ;
             rdfs:comment "Service dates specified using a weekly schedule with start and end dates."@en ,
                          "Un gtfs:Service indica el conjunto de fechas en el que el servicio está disponible para una o más rutas"@es ;
             rdfs:label "Service"@en ,
                        "Servicio"@es ;
             rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#calendar_dates_fields> ,
                          <https://developers.google.com/transit/gtfs/reference#calendar_fields> .


gtfs:ServiceRule rdf:type owl:Class ;
                 rdfs:comment "One or more service rules define a set of dates"@en ,
                              "Una o más reglas de servicio definen un conjunto de fechas"@es ;
                 rdfs:label "Service rule"@en ,
                            "Regla de servicio"@es .                                


gtfs:Shape rdf:type owl:Class ;
           rdfs:comment "A polygon formed by gtfs:ShapePoints"@en ,
                        "Un polígono o línea formado por gtfs:ShapePoints"@es ;
           rdfs:label "Shape"@en ,
                      "forma del recorrido, trayecto, itinerario"@es ;
           rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#shapes_fields> .


gtfs:ShapePoint rdf:type owl:Class ;
                rdfs:comment "A geographic point within a gtfs:Shape"@en ,
                             "Punto geográfico dentro de un recorrido (gtfs:Shape)"@es ;
                rdfs:label "Punto de recorrido"@es ,
                           "Shape Point"@en ;
                rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#shapes_fields> .


gtfs:Station rdf:type owl:Class ;
             rdfs:subClassOf gtfs:Location ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty gtfs:latitude ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty gtfs:longitude ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]  ;
             rdfs:comment "A physical structure or area that contains one or more platform."@en ,
                          "Estructura física o área que contiene una o más paradas."@es ;
             rdfs:label "Estación"@es ,
                        "Station"@en ;
             rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stops_fields> .


gtfs:StationEntrance rdf:type owl:Class ;
                     rdfs:subClassOf gtfs:Location ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty gtfs:latitude ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty gtfs:longitude ;
                          owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                        ]  ;
                     rdfs:comment "A location where passengers can enter or exit a station from the street. If an entrance/exit belongs to multiple stations, it can be linked by pathways to both, but the data provider must pick one of them as parent."@en ;
                     rdfs:label "Station Entrance"@en .


gtfs:GenericNode rdf:type owl:Class ;
                 rdfs:subClassOf gtfs:Location ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty gtfs:latitude ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty gtfs:longitude ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    ]  ;
                 rdfs:comment "A location within a station, not matching any other location type, which can be used to link together pathways."@en ;
                 rdfs:label "Generic Node"@en .


gtfs:BoardingArea rdf:type owl:Class ;
                  rdfs:subClassOf gtfs:Location ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty gtfs:latitude ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    ] ,
                    [ rdf:type owl:Restriction ;
                      owl:onProperty gtfs:longitude ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    ]  ;
                  rdfs:comment "A specific location on a platform, where passengers can board and/or alight vehicles."@en ;
                  rdfs:label "Boarding Area"@en .


gtfs:Stop rdf:type owl:Class ;
          rdfs:subClassOf gtfs:Location ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty gtfs:latitude ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty gtfs:longitude ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]  ;
          rdfs:comment "A stop is a physical location where a vehicle stops or leaves. Multiple routes may use the same stop."@en ,
                       "Un lugar donde los pasajeros suben o bajan de un vehículo de transporte público."@es ;
          rdfs:label "Parada"@es ,
                     "Stop"@en ;
          rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stops_fields> .



gtfs:StopTime rdf:type owl:Class ;
              rdfs:comment "Describe una hora de parada como parte de un viaje"@es ,
                           "Describes a stop time as part of a trip"@en ;
              rdfs:label "Hora de parada"@es ,
                         "Stop time"@en ;
              rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#stop_times_fields> .


gtfs:Trip rdf:type owl:Class ;
          rdfs:comment "A collection of gtfs:StopTimes followed by a transit vehicle"@en ,
                       "Trayecto de dos o más paradas realizadas por un vehículo que se produce a una gtfs:StopTime"@es ;
          rdfs:label "Trip"@en ,
                     "Viaje"@es ;
          rdfs:seeAlso <https://developers.google.com/transit/gtfs/reference#trips_fields>.

