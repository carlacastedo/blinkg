@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ex: <http://example.org/countries/> .
@prefix schema: <http://schema.org/> .
@prefix dct: <http://purl.org/dc/terms/> .

<http://example.com/base/TriplesMap1> a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "country_info.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;

    rr:subjectMap [
        rr:template "http://example.org/countries/{Country Code}" ;
        rr:class schema:Country
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:identifier ;
        rr:objectMap [ rml:reference "Country Code" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:name ;
        rr:objectMap [ rml:reference "Name" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate dct:identifier ;
        rr:objectMap [ rml:reference "ISO 3166" ]
    ] .
