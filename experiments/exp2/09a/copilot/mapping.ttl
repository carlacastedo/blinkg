@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#StudentMapping>
    a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "student.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;
    rr:subjectMap [
        rr:template "http://example.org/student_{ID}" ;
        rr:class ex:Person ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate ex:id ;
        rr:objectMap [ rml:reference "ID" ; rr:datatype xsd:integer ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate ex:name ;
        rr:objectMap [ rml:reference "Name" ; rr:datatype xsd:string ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate ex:practises ;
        rr:objectMap [ rr:template "http://example.org/{Sport}" ] ;
    ] .

<#SportMapping>
    a rr:TriplesMap ;
    rml:logicalSource [
        rml:source "sport.csv" ;
        rml:referenceFormulation ql:CSV
    ] ;
    rr:subjectMap [
        rr:template "http://example.org/sport_{ID}" ;
        rr:class ex:Sport ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate ex:sportid ;
        rr:objectMap [ rml:reference "ID" ; rr:datatype xsd:integer ] ;
    ] ;
    rr:predicateObjectMap [
        rr:predicate ex:sportname ;
        rr:objectMap [ rml:reference "Name" ; rr:datatype xsd:string ] ;
    ] .
