@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .

ex:Person a owl:Class ;
   rdf:label "person" .

ex:Sport a owl:Class ;
   rdfs:label "sport" .

ex:City a owl:Class ;
   rdf:label "city" .

ex:id a owl:DatatypeProperty ;
   rdfs:label "id" ;
   rdfs:range xsd:integer ;
   rdfs:domain ex:Person .

ex:name a owl:DatatypeProperty ;
   rdfs:label "name" ;
   rdfs:range xsd:string ;
   rdfs:domain ex:Person .

ex:firstname a owl:DatatypeProperty ;
   rdfs:label "name" ;
   rdfs:range xsd:string ;
   rdfs:domain ex:Person ;
   rdfs:subClassOf ex:name .

ex:lastname a owl:DatatypeProperty ;
   rdfs:label "name" ;
   rdfs:range xsd:string ;
   rdfs:domain ex:Person ;
   rdfs:subClassOf ex:name .

ex:sportid a owl:DatatypeProperty ;
   rdfs:label "sportid" ;
   rdfs:range xsd:integer ;
   rdfs:domain ex:Sport .

ex:sportname a owl:DatatypeProperty ;
   rdfs:label "sportname" ;
   rdfs:range xsd:langString ;
   rdfs:domain ex:Sport .

ex:practises a owl:ObjectProperty ;
   rdfs:label "practises" ;
   rdfs:range ex:Sport ;
   rdfs:domain ex:Person .

ex:owes a owl:DatatypeProperty ;
   rdfs:label "owes" ;
   rdfs:range xsd:double ;
   rdfs:domain ex:Person .

ex:lives a owl:ObjectProperty ;
   rdfs:label "lives" ;
   rdfs:range xsd:string ;
   rdfs:domain ex:Person .

ex:Mapping a rr:TriplesMap;
   rml:logicalSource[
      rml:source "student.csv" ;
      rml:referenceFormulation ql:CSV
   ];
   rr:subjectMap [
      rr:template "http://example.org/person/{ID}" ;
      rr:class ex:Person
   ];
   rr:predicateObjectMap [
      rr:predicate ex:id;
      rr:objectMap [rml:reference "ID"];
   ];
   rr:predicateObjectMap [
      rr:predicate ex:name;
      rr:objectMap [rml:reference "Name"];
   ].

ex:MappingSport a rr:TriplesMap;
   rml:logicalSource[
      rml:source "student.csv" ;
      rml:referenceFormulation ql:CSV
   ];
   rr:subjectMap [
      rr:template "http://example.org/sport/{Sport}" ;
      rr:class ex:Sport
   ];
   rr:predicateObjectMap [
      rr:predicate ex:practises;
      rr:objectMap [rml:reference "Sport"];
   ].
