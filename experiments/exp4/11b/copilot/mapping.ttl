@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.org/#> .

<#StudentMapping>
  rml:logicalSource [
    rml:source "student.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/Students/Student"
  ];
  rr:subjectMap [
    rr:template "http://example.org/#{ID}";
    rr:class ex:Person
  ];
  rr:predicateObjectMap [
    rr:predicate ex:id;
    rr:objectMap [ rml:reference "ID" ]
  ];
  rr:predicateObjectMap [
    rr:predicate ex:firstname;
    rr:objectMap [ rml:reference "FirstName" ]
  ];
  rr:predicateObjectMap [
    rr:predicate ex:lastname;
    rr:objectMap [ rml:reference "LastName" ]
  ].

<#SportMapping>
  rml:logicalSource [
    rml:source "sport.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/Sports/Sport"
  ];
  rr:subjectMap [
    rr:template "http://example.org/#{ID}";
    rr:class ex:Sport
  ];
  rr:predicateObjectMap [
    rr:predicate ex:sportid;
    rr:objectMap [ rml:reference "ID" ]
  ];
  rr:predicateObjectMap [
    rr:predicate ex:sportname;
    rr:objectMap [ rml:reference "Description" ]
  ].

<#StudentSportMapping>
  rml:logicalSource [
    rml:source "student_sport.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/links/link"
  ];
  rr:subjectMap [
    rr:template "http://example.org/#{ID_Student}_#{ID_Sport}"
  ];
  rr:predicateObjectMap [
    rr:predicate ex:practises;
    rr:objectMap [ rml:reference "ID_Sport" ]
  ].
