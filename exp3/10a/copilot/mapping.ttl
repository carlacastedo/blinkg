@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.org/#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMap1>
    a rr:TriplesMap;
    rml:logicalSource [
        rml:source "country_info.json";
        rml:referenceFormulation ql:JSONPath;
        rml:iterator "$.countries[*]"
    ];

    rr:subjectMap [
        rr:template "http://example.org/#{Country Code}";
        rr:class ex:Country
    ];

    rr:predicateObjectMap [
        rr:predicate ex:id;
        rr:objectMap [ rml:reference "Country Code"; rr:datatype xsd:int ]
    ];

    rr:predicateObjectMap [
        rr:predicate ex:name;
        rr:objectMap [ rml:reference "Name"; rr:datatype xsd:string ]
    ];

    rr:predicateObjectMap [
        rr:predicate ex:iso3166;
        rr:objectMap [ rml:reference "ISO 3166"; rr:datatype xsd:string ]
    ].
